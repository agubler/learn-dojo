{
	"build-app": {
		"build-time-render": {
			"root": "root",
			"paths": [
				"build-time-rendering-in-dojo",
				"building-a-simple-app-in-dojo",
				"creating-a-datepicker-with-dojo",
				"dojo-cli-template-app",
				"dojo-containers",
				"dojo-from-the-blocks",
				"intro-to-the-dojo-registry",
				"intro-to-the-dojo-router",
				"maintain-state-with-dojo-stores",
				"style-dojo-widgets",
				"testing-with-dojo",
				"up-and-running-with-dojo-cli",
				"watch-for-property-changes-in-widgets",
				"web-components-with-dojo",
				"."
			],
			"puppeteerOptions": {
				"args": [ "--no-sandbox", "--disable-setuid-sandbox" ]
			}
		},
		"pwa": {
			"manifest": {
				"name": "dojo-esri-loader",
				"short_name": "dojo-esri-loader",
				"description": "dojo-esri-loader",
				"background_color": "#ffffff",
				"theme_color": "#009dff",
				"icons": [
					{
						"src": "src/assets/logo.png",
						"sizes": [ 128, 256, 512 ]
					}
				]
			},
			"serviceWorker": {
				"clientsClaim": true,
				"routes": [
					{
						"urlPattern": "https://learn-dojo.com",
						"strategy": "networkFirst",
						"expiration": {
							"maxEntries": 25
						}
					}
				]
			}
		}
	},
	"test-intern": {},
	"create-app": {},
	"create-widget": {}
}
